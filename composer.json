{
    "name": "onesignal/wordpress-plugin",
    "description": "OneSignal WordPress Plugin",
    "type": "wordpress-plugin",
    "require": {
        "php": ">=7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "yoast/phpunit-polyfills": "^1.0"
    },
    "autoload-dev": {
        "classmap": [
            "tests/unit"
        ]
    },
    "scripts": {
        "test": "phpunit",
        "test:unit": "phpunit --testsuite unit"
    }
}
